<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <TargetFramework>netcoreapp2.0</TargetFramework>
    <Authors>Matt Scheetz</Authors>
    <PackageProjectUrl>https://github.com/mscheetz/KuCoinApi.NetCore</PackageProjectUrl>
    <RepositoryUrl>https://github.com/mscheetz/KuCoinApi.NetCore</RepositoryUrl>
    <PackageTags>kucoin cryptocurrency exchange trading api net core bitcoin</PackageTags>
    <Description>Api library for accessing the KuCoin exchange Api</Description>
    <Copyright>2018 Matt Scheetz</Copyright>
    <PackageReleaseNotes>Withdrawal endpoints plus deposit/withdraw history</PackageReleaseNotes>
    <IsPackable>true</IsPackable>
    <Version>1.4.2</Version>
    <PackageLicenseUrl>https://github.com/mscheetz/KuCoinApi.NetCore/blob/master/LICENSE</PackageLicenseUrl>
  </PropertyGroup>

  <ItemGroup>
    <PackageReference Include="DateTimeHelpers" Version="1.0.0" />
    <PackageReference Include="FileRepository" Version="1.0.0" />
    <PackageReference Include="Newtonsoft.Json" Version="11.0.2" />
    <PackageReference Include="RESTApiAccess" Version="1.0.0" />
    <PackageReference Include="xunit" Version="2.4.0" />
    <PackageReference Include="xunit.runner.visualstudio" Version="2.4.0" />
  </ItemGroup>

  <ItemGroup>
    <Folder Include="Core.Interface\" />
  </ItemGroup>

</Project>
